@model UsedSale_Car.Models.Classes.AboutUs
@{
    ViewBag.Title = "NewAbout";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Yeni About Ekle</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.Title)
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Description)
    @Html.TextBoxFor(x => x.Description, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.FotoUrl)
    @Html.TextBoxFor(x => x.FotoUrl, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">About Ekle</button>
    <br />
    <br />
</form>